<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nithinkumar.scorer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/appicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartGameActivity"/>
        <activity android:name=".BasketBallMainActivity"/>
        <activity android:name=".FootBallMainActivity"/>
        <activity android:name=".CricketMainActivity"/>
        <activity android:name=".HockeyMainActivity"/>
    </application>

</manifest>